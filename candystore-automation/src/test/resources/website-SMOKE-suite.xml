<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="website-SMOKE-suite">
	<test name="All tests with the group 'smoke' ">
		<groups>
			<run>
				<include name="smoke" />
			</run>
		</groups>
		<packages>
			<package name="com.agiletestingframework.candystore.common" />
			<package name="com.agiletestingframework.candystore.website" />
		</packages>
	</test>
</suite>
